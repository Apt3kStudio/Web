{
  "version": "1.0",
  "defaultProvider": "filesystem",
  "libraries": [
    {
      "library": "wwwroot/css/site.css",
      "destination": "wwwroot/css/",
      "files": [
        "site.min.css"
      ]
    },
    {
      "library": "wwwroot/js/site.js",
      "destination": "wwwroot/js/",
      "files": [
        "site.min.js"
      ]
    },
    {
      "provider": "cdnjs",
      "library": "summernote@0.8.12",
      "destination": "wwwroot/js/summernote/"
    },
    {
      "provider": "cdnjs",
      "library": "font-awesome@5.8.2",
      "destination": "wwwroot/css/font-awesome/"
    },
    {
      "provider": "cdnjs",
      "library": "codemirror@5.47.0",
      "destination": "wwwroot/lib/codemirror/"
    },
    {
      "provider": "cdnjs",
      "library": "animate.css@3.7.2",
      "destination": "wwwroot/lib/animate.css/"
    }
  ]
}